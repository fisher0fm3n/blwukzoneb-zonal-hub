rom \"vuex\";\r\n\r\nexport default {\r\n  name: \"itemModal\",\r\n  data() {\r\n    return {\r\n      itemName: null,\r\n      itemBarcode: null,\r\n      itemCategory: null,\r\n      itemDescription: null,\r\n      itemDate: null,\r\n      itemTime: null,\r\n      itemImages: [],\r\n      ownerName: null,\r\n      adminName: null,\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"TOGGLE_ITEM\"]),\r\n    closeAddItem() {\r\n      this.TOGGLE_ITEM();\r\n    },\r\n\r\n    async uploadItem() {\r\n      if (this.itemImages.length <= 0) {\r\n        alert(\"Please upload images of the item\");\r\n        return;\r\n      }\r\n\r\n      const dataBase = db.collection(\"items\").doc();\r\n\r\n      await dataBase.set({\r\n        itemName: this.itemName,\r\n        itemBarcode: this.itemBarcode,\r\n        itemCategory: this.itemCategory,\r\n        itemDescription: this.itemDescription,\r\n        itemDate: this.itemDate,\r\n        itemTime: this.itemTime,\r\n        itemImages: this.itemImages,\r\n        ownerName: this.ownerName,\r\n        adminName: this.adminName,\r\n      });\r\n    },\r\n\r\n    submitForm() {\r\n      this.uploadItem();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.item-wrap {\r\n  position: fixed;\r\n  top: 0;\r\n  background-color: transparent;\r\n  height: 100%;\r\n\r\n  .item-content {\r\n    position: relative;\r\n    padding: 56px;\r\n    max-width: 700px;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: #ff9a22;\r\n    color: #fff;\r\n\r\n    h1 {\r\n      text-align: left;\r\n      font-weight: bold;\r\n      margin-bottom: 1rem;\r\n      color: #fff;\r\n    }\r\n  }\r\n}\r\n.add-button {\r\n  cursor: pointer;\r\n  font-size: 0.9rem;\r\n  font-weight: 400;\r\n  border-radius: 3rem;\r\n  padding: 0.6rem 1.2rem;\r\n  background-color: #ff9a22;\r\n  color: #fff;\r\n  .inner-button {\r\n    color: #ff9a22;\r\n    background-color: #fff;\r\n    border-radius: 100%;\r\n    margin-right: 0.6rem;\r\n    min-width: 2rem;\r\n    min-height: 2rem;\r\n  }\r\n}\r\n</style>\r\n"]}]}