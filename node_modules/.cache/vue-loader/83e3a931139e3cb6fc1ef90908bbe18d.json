/Users/dpitt/Workspace/blwukzoneb-zonal-hub/src/components/ItemModal.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div @click=\"checkClick\" ref=\"itemWrap\" class=\"item-wrap d-flex flex-column\">\r\n    <form @submit.prevent=\"submitForm\" class=\"item-content\">\r\n      <h1>New Item</h1>\r\n\r\n      <!-- Item Info -->\r\n      <div class=\"item-info d-flex flex-column\">\r\n        <div class=\"input d-flex flex-column\">\r\n          <label for=\"itemName\">Item Name</label>\r\n          <input required type=\"text\" id=\"itemName\" v-model=\"itemName\" />\r\n        </div>\r\n        <div class=\"item-details d-flex justify-content-between\">\r\n          <div class=\"input d-flex flex-column\">\r\n            <label for=\"itemBarcode\">barcode</label>\r\n            <input\r\n              required\r\n              type=\"text\"\r\n              id=\"itemBarcode\"\r\n              v-model=\"itemBarcode\"\r\n            />\r\n          </div>\r\n          <div class=\"input d-flex flex-column\">\r\n            <label for=\"itemCategory\">Category</label>\r\n            <select id=\"itemCategory\" v-model=\"itemCategory\">\r\n              <option value=\"camera\">Camera</option>\r\n              <option value=\"light\">Light</option>\r\n              <option value=\"cable\">Cable</option>\r\n              <option value=\"device\">Mobile/Computer Device</option>\r\n              <option value=\"peripheral\">Peripheral</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <div class=\"item-description\">\r\n          <div class=\"input d-flex flex-column\">\r\n            <label for=\"itemDescription\">Description</label>\r\n            <textarea\r\n              required\r\n              id=\"itemDescription\"\r\n              rows=\"10\"\r\n              cols=\"30\"\r\n              v-model=\"itemDescription\"\r\n            ></textarea>\r\n          </div>\r\n        </div>\r\n        <div class=\"input d-flex flex-column\">\r\n          <label for=\"ownerName\">Owner Name</label>\r\n          <input required type=\"text\" id=\"ownerName\" v-model=\"ownerName\" />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Item Admin -->\r\n      <div class=\"item-admin d-flex flex-column\">\r\n        <div class=\"item-admin-details d-flex justify-content-between\">\r\n          <div class=\"input d-flex flex-column\">\r\n            <label for=\"itemDate\">Item Date</label>\r\n            <input disabled type=\"text\" id=\"itemDate\" v-model=\"itemDate\" />\r\n          </div>\r\n          <div class=\"input d-flex flex-column\">\r\n            <label for=\"adminName\">Admin Name</label>\r\n            <input disabled type=\"text\" id=\"adminName\" v-model=\"adminName\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Save/Exit Buttons -->\r\n      <div class=\"save d-flex\">\r\n        <button class=\"add-button\">Add item</button>\r\n      </div>\r\n\r\n      <div class=\"canel d-flex\">\r\n        <button @click=\"closeAddItem\" class=\"add-button\">Cancel</button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport db from \"../firebase/firebaseinit\";\r\nimport { mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"itemModal\",\r\n  data() {\r\n    return {\r\n      itemName: null,\r\n      itemBarcode: null,\r\n      itemCategory: null,\r\n      itemDescription: null,\r\n      itemDate: null,\r\n      itemTime: null,\r\n      itemImages: [],\r\n      ownerName: null,\r\n      adminName: null,\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"TOGGLE_ITEM\"]),\r\n    closeAddItem() {\r\n      this.TOGGLE_ITEM();\r\n    },\r\n\r\n    async uploadItem() {\r\n      if (this.itemImages.length <= 0) {\r\n        alert(\"Please upload images of the item\");\r\n        return;\r\n      }\r\n\r\n      const dataBase = db.collection(\"items\").doc();\r\n\r\n      await dataBase.set({\r\n        itemName: this.itemName,\r\n        itemBarcode: this.itemBarcode,\r\n        itemCategory: this.itemCategory,\r\n        itemDescription: this.itemDescription,\r\n        itemDate: this.itemDate,\r\n        itemTime: this.itemTime,\r\n        itemImages: this.itemImages,\r\n        ownerName: this.ownerName,\r\n        adminName: this.adminName,\r\n      });\r\n    },\r\n\r\n    submitForm() {\r\n      this.uploadItem();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.item-wrap {\r\n  position: fixed;\r\n  top: 0;\r\n  background-color: transparent;\r\n  height: 100%;\r\n\r\n  .item-content {\r\n    position: relative;\r\n    padding: 56px;\r\n    max-width: 700px;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: #ff9a22;\r\n    color: #fff;\r\n\r\n    h1 {\r\n      text-align: left;\r\n      font-weight: bold;\r\n      margin-bottom: 1rem;\r\n      color: #fff;\r\n    }\r\n  }\r\n}\r\n.add-button {\r\n  cursor: pointer;\r\n  font-size: 0.9rem;\r\n  font-weight: 400;\r\n  border-radius: 3rem;\r\n  padding: 0.6rem 1.2rem;\r\n  background-color: #ff9a22;\r\n  color: #fff;\r\n  .inner-button {\r\n    color: #ff9a22;\r\n    background-color: #fff;\r\n    border-radius: 100%;\r\n    margin-right: 0.6rem;\r\n    min-width: 2rem;\r\n    min-height: 2rem;\r\n  }\r\n}\r\n</style>\r\n"]}]}